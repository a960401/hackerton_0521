
		
<% @aboutuser.each do |u|%>  

    <div class="container">
             <div class="form-group">  
                <label for="name">이름</label>
                <div class="form-control"><%=u.username%></div>
              </div>
              
              <div class="form-group">  
                <label for="number">연락처</label>
                <div class="form-control"><%=u.phone_number%></div>
              </div>
              
              <div class="form-roup">  
                <label for="usertype">이사종류</label>
                <div class="form-control"><%=u.move%></div>
              </div>
              
              <div class="form-group">  
                <label for="depart">출발지</label>
                <div class="form-control"><%=u.depart_country%>&nbsp; <%=u.depart_region%></div>
              </div>
              
              <div class="form-group">  
                <label for="arrive">도착지</label>
                <div class="form-control"><%=u.arrive_country%>&nbsp; <%=u.arrive_region%></div>
              </div>
              
              <div class="form-group">
                <label for="time">날짜</label>
                  <div class="form-control"><%=u.userdate%>&nbsp;<%=u.usertime.to_s(:time)%></div>
              </div>
              
              <div class="form-group">  
                <label for="arrive">이사물품</label></label>
                <div class="form-control">
                  <%=u.question_1%>
                </div>
              </div>
    </div>
              
    
<%end%>