<section class="container" id="main">

<!-- Start Survey container -->
<div id="survey_container">
 <% u = @aboutuser %>  
    <br><br>
    
    <div class="container">
             <div class="form-group" style="width: 80%; margin-left:auto; margin-right:auto">  
                <label for="name">이름</label>
                <div class="form-control required"><%=u.username%></div>
              </div>
              
              <div class="form-group" style="width: 80%; margin-left:auto; margin-right:auto">  
                <label for="number">연락처</label>
                <div class="required form-control"><%=u.phone_number%></div>
              </div>
              
              <div class="form-group" style="width: 80%; margin-left:auto; margin-right:auto"ㅁ>  
                <label for="usertype">이사종류</label>
                <div class="required form-control"><%=u.move%></div>
              </div>
              
              <div class="form-group" style="width: 80%; margin-left:auto; margin-right:auto">  
                <label for="depart">출발지</label>
                <div class="required form-control"><%=u.depart_country%>&nbsp; <%=u.depart_region%></div>
              </div>
              
              <div class="form-group" style="width: 80%; margin-left:auto; margin-right:auto">  
                <label for="arrive">도착지</label>
                <div class="required form-control"><%=u.arrive_country%>&nbsp; <%=u.arrive_region%></div>
              </div>
              
              <div class="form-group" style="width: 80%; margin-left:auto; margin-right:auto">
                <label for="time">날짜</label>
                  <div class="required form-control"><%=u.userdate%>&nbsp;<%=u.usertime.to_s(:time)%></div>
              </div>
              
              
              <!--<div class="form-group" style="width: 80%; margin-left:auto; margin-right:auto">-->
              <!--  <label for="password">비밀번호</label>-->
              <!--    <div class="required form-control"><%#=u.userpassword%>&nbsp;<%#=u.userpassword%></div>-->
              <!--</div>-->
              
              <div class="form-group" style="width: 80%; margin-left:auto; margin-right:auto">  
                <label for="arrive">이사물품</label></label>
                <div class="required form-control">
                  <%=u.question_1%>
                </div>
              </div>
    </div>
    <%# end %>
	  <br><br>
		</form>
    
</div><!-- end Survey container -->

